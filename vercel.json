{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "index.js" }
    ],
    "env": {
      "PRISMA_GENERATE": "npx prisma generate"
    },
    "build": {
      "env": {
        "PRISMA_FORCE_NAPI": "true"
      },
      "scripts": {
        "build": "npm run build",
        "postinstall": "${PRISMA_GENERATE}"
      }
    }
  }
  